
/* 定义公共 */

  /* 浮动 */
  .fl{
    float: left;
  }
  .fr{
    float: right;
  }
  .clear::after{
    content: "";
    display:block;
    clear: both;
  }


  /* 定位 */
  .pr{
    position: relative;
  }
  .pa{
    position: absolute;
  }


  .ver-middle{
    vertical-align: middle;
  }


  /* 盒子模型 */

  .box-size{
    box-sizing: border-box;
  }
  .border-common{
    border: 1px solid #E4E4E4
  }
  .border-top{
    border-top: 1px solid #ebeef5
  }
  .border-left{
    border-left: 1px solid #ebeef5
  }
  .cursor{
    cursor: pointer;
  }
  .no-cursor{
    cursor: text!important;
  }
  
  .overflow-y{
    overflow-y: auto; 
  }

  .no-data{
    text-indent: 20px;
  }


/* 定义公共的按钮 */
.my-btn{
  background-color: #fff;
  color: #333;
  border: 1px solid #CCCCCC;
  cursor: pointer;
  min-width: 70px;
  padding: 4px 5px;
  height: 30px;
  line-height: 30px;
}
.my-btn:hover{
  background-color: #e0e0e0
}
.my-btn:active{
  background-color: #ccc
}

.submit-btn{
  background-color: #0099FF;
  color: #fff;
  border-radius: 5px;
  border: none;
  min-width: 70px;
  padding: 4px 5px;
  height: 30px;
  line-height: 30px;
}
.submit-btn:hover{
  background-color: #0189e3
}
.no-pass-btn{
  background-color: rgb(255, 102, 102);
  color: #fff;
  border-radius: 5px;
  border: none;
  min-width: 70px;
  padding: 4px 5px;
  height: 30px;
  line-height: 30px;
}
.no-pass-btn:hover{
  background-color: rgb(232, 74, 74)
}
/* 宽高100% */
.spread-out{
  width: 100%;
  height: 100%;
}

.color-white{
  color: #fff;
}
.color-red{
  color: red;
}
.color-blue{
  color: #0099FF;
}
.color-green{
  color: #67c23a
}
/* 超出三点 */
.white-space{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

/* 侧边导航三角 */
.triangle{
  position: relative
}
.triangle::after{
  border: 10px solid transparent;
  border-right: 10px solid rgba(243,243,243,1);;
  width: 0;
  height: 0;
  position: absolute;
  left: 100%;
  transform: translateX(-100%);
  content: ' '
}



.el-select-dropdown.el-popper.is-multiple{
  width: 180px!important;
}